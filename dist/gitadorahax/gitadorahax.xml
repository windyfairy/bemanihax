<?xml version="1.0" encoding="shift-jis"?>
<gitadorahax>
    <!-- Changes the files read so omnimix can live alongside base game without modifying databases. -->
    <!-- New mdb_*.xml: odb_*.xml -->
    <!-- New notes_info.xml: omni_notes.xml -->
    <!-- New phrase_address_list.xml: omni_phrase_address.xml -->
    <omnimix __type="bool">0</omnimix>

    <!-- Freeze timer on song select screen. -->
    <timer_freeze __type="bool">0</timer_freeze>

    <!-- Freeze stage counter. Allows you to play an infinite number of levels, but does not allow you to save scores on networks. -->
    <stage_freeze __type="bool">0</stage_freeze>

    <!-- Unlock all songs. Not recommended for networks. -->
    <all_music __type="bool">0</all_music>

    <!-- If you don't know, don't enable it. -->
    <scorecard_hook __type="bool">0</scorecard_hook>

    <!-- Display slow/fast/average counts in the bottom right and left of the screen where PASELI text would normally be. -->
    <display_timing_stats __type="bool">0</display_timing_stats>

    <!-- Cabinet types: 0/1 = DX, 2 = SD, 3 = White cab -->
    <machine_type __type="s32">0</machine_type>
</gitadorahax>
